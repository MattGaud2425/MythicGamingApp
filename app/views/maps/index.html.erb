<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Name:</th>
      <th>Game Title</th>
      <th>Bracket Title</th>
    </tr>
  </thead>

  <tbody>
    <% @maps.each do |map| %>
      <tr>
        <td><%= map.name %></td>
        <td><%= map.bracket.game.title %></td>
        <td><%= map.bracket.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Map", new_game_bracket_map_path(@game, @bracket) %>
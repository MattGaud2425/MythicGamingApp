<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<p><%= link_to "Create New User", new_user_path %></p>

<% @users.each do |x| %>
	<p><%= x.email + ", " %>,
	<%= x.admin %>,
	<%= link_to "Edit", edit_user_path(x.id) %>,
	<%= link_to "View", user_path(x.id) %>,
	<%= link_to "Delete"  %>

<% end %>
